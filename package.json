{
  "name": "mindvibe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "analyze": "ANALYZE=true next build",
    "prepare": "command -v pre-commit >/dev/null 2>&1 && pre-commit install || echo 'pre-commit not installed, skipping hook setup'",
    "aider": "bash scripts/run-aider.sh",
    "aider:ps": "pwsh -File scripts/run-aider.ps1",
    "viyoga:index": "node scripts/build-viyoga-index.mjs"
  },
  "engines": {
    "node": ">=20.0.0 <25.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.7.9",
    "bcryptjs": "file:vendor/bcryptjs",
    "canvas-confetti": "^1.9.4",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.23.25",
    "idb": "^8.0.3",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.468.0",
    "next": "16.1.1",
    "next-intl": "^4.5.8",
    "openai": "^6.18.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^10.0.0",
    "recharts": "^2.15.4",
    "simplebar": "^6.3.0",
    "simplebar-react": "^2.4.3",
    "typescript": "^5.9.3",
    "web-vitals": "^4.2.4",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.58.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/dom-speech-recognition": "^0.0.7",
    "@types/node": "^24.10.13",
    "@types/papaparse": "^5.5.1",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.15",
    "vitest": "^4.0.17"
  }
}

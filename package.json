{
  "name": "mindvibe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "test:ui": "vitest --ui",
    "prepare": "command -v pre-commit >/dev/null 2>&1 && pre-commit install || echo 'pre-commit not installed, skipping hook setup'",
    "aider": "bash scripts/run-aider.sh",
    "aider:ps": "pwsh -File scripts/run-aider.ps1"
  },
  "engines": {
    "node": ">=20.0.0 <25.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.7.9",
    "bcryptjs": "file:vendor/bcryptjs",
    "canvas-confetti": "^1.9.4",
    "date-fns": "^2.30.0",
    "firebase": "^12.6.0",
    "framer-motion": "^12.23.25",
    "jspdf": "^3.0.4",
    "lottie-react": "^2.4.1",
    "next": "16.1.1",
    "next-intl": "^4.5.8",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^10.0.0",
    "react-use-gesture": "^9.1.3",
    "recharts": "^2.15.4",
    "simplebar": "^6.3.0",
    "simplebar-react": "^2.4.3",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/dom-speech-recognition": "^0.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/papaparse": "^5.5.1",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.57.1",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
